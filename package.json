{
    "name": "csharp-api-console",
    "displayName": "%extension.displayName%",
    "description": "%extension.description%",
    "version": "1.0.19",
    "publisher": "dankit",
    "icon": "resources/icons/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/wjire/csharp-api-console"
    },
    "bugs": {
        "url": "https://github.com/wjire/csharp-api-console/issues"
    },
    "homepage": "https://github.com/wjire/csharp-api-console#readme",
    "license": "MIT",
    "keywords": [
        "api",
        "api testing",
        "api client",
        "rest",
        "rest api",
        "restful",
        "http",
        "http client",
        "csharp",
        "c#",
        "aspnet",
        "aspnet core",
        "asp.net",
        "dotnet",
        ".net",
        "controller",
        "endpoint",
        "web api",
        "codelens",
        "testing",
        "debug",
        "debugger",
        "postman",
        "insomnia",
        "route",
        "routes",
        "api explorer"
    ],
    "galleryBanner": {
        "color": "#007ACC",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Programming Languages",
        "Testing",
        "Debuggers"
    ],
    "activationEvents": [
        "onLanguage:csharp"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "csharpApiConsole.testEndpoint",
                "title": "%command.testEndpoint.title%"
            }
        ],
        "configuration": {
            "title": "%configuration.title%",
            "properties": {
                "csharpApiConsole.codeLensDebounceDelay": {
                    "type": "number",
                    "default": 300,
                    "minimum": 0,
                    "maximum": 1000,
                    "description": "%configuration.codeLensDebounceDelay.description%"
                },
                "csharpApiConsole.codeLensCacheTtlSeconds": {
                    "type": "number",
                    "default": 120,
                    "minimum": 0,
                    "maximum": 3600,
                    "description": "%configuration.codeLensCacheTtlSeconds.description%"
                },
                "csharpApiConsole.codeLensCacheMaxEntries": {
                    "type": "number",
                    "default": 100,
                    "minimum": 1,
                    "maximum": 200,
                    "description": "%configuration.codeLensCacheMaxEntries.description%"
                },
                "csharpApiConsole.defaultApiVersion": {
                    "type": "string",
                    "default": "1.0",
                    "description": "%configuration.defaultApiVersion.description%"
                },
                "csharpApiConsole.requestHistoryLimit": {
                    "type": "number",
                    "default": 10,
                    "minimum": 1,
                    "maximum": 20,
                    "description": "%configuration.requestHistoryLimit.description%"
                },
                "csharpApiConsole.requestHistoryEnabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "%configuration.requestHistoryEnabled.description%"
                },
                "csharpApiConsole.requestHistoryTtlDays": {
                    "type": "number",
                    "default": 3,
                    "description": "%configuration.requestHistoryTtlDays.description%"
                },
                "csharpApiConsole.requestTimeoutSeconds": {
                    "type": "number",
                    "default": 0,
                    "description": "%configuration.requestTimeoutSeconds.description%"
                },
                "csharpApiConsole.largeResponseThresholdKb": {
                    "type": "number",
                    "default": 1024,
                    "description": "%configuration.largeResponseThresholdKb.description%"
                },
                "csharpApiConsole.maxResponseLineNumbers": {
                    "type": "number",
                    "default": 2000,
                    "description": "%configuration.maxResponseLineNumbers.description%"
                },
                "csharpApiConsole.requestHistoryMaxBodyKb": {
                    "type": "number",
                    "default": 32,
                    "description": "%configuration.requestHistoryMaxBodyKb.description%"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3",
        "@vscode/test-electron": "^2.3.8"
    }
}